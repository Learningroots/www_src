@{
    Layout = "~/Views/Shared/_ColumnsOne.cshtml";
    Html.AppendScriptParts("https://maps.googleapis.com/maps/api/js?v=3.exp");
    Html.AppendScriptParts("https://cdnjs.cloudflare.com/ajax/libs/lodash.js/3.10.1/lodash.min.js");

    Html.AddTitleParts("Retailers");

    var marker = "~/Themes/DefaultClean/Content/images/marker-lr.png";
    var jumptotop = "~/Themes/DefaultClean/Content/images/jumptotop.png";
}

<style type="text/css">
	#map-canvas {
        height: 400px;
        margin: 0;
        margin-top: 8px;
        padding: 0;
        width: 100%;
	}
	.master-wrapper-main {
		padding-top: 0;
	}
	.retailers-page {
		margin-top: 20px;
	}
	#back-to-top {
        background: url("@Url.Content(jumptotop)") no-repeat 0 0;
        background-size: 60px;
        position: fixed;
        bottom: 40%;
        right: 40px;
        z-index: 9999;
        width: 60px;
        height: 60px;
        cursor: pointer;
        border: 0;
        transition: opacity 0.2s ease-out;
        opacity: 0;
        font-size: 52px;
	}
	#back-to-top.show:hover {
	   opacity: 1;
	}
	#back-to-top.show {
	   opacity: 0.8;
	}
</style>

<a href="#" id="back-to-top" title="Back to top"></a>

<div id="map-canvas"></div>

<div class="retailers-page">
    <div class="page-body">
    <div class="wrapper">

    	<span class="page-title">
	    	<h1 class="retailers-title">
	    		<span class="girl-pic"></span><span class="js-num-retailers"></span> retailers found Worldwide</span>
	    	</h1>
	    </span>

			<ul class="js-retailers-links retailers-links"></ul>
			<ul class="js-retailers-list retailers-list"></ul>
		</div>
	</div>
</div>




<script>

$('.retailers-page').closest('.wrapper').removeClass('wrapper');

var map;
var icon = "@Url.Content(marker)";

function initialize() {
	$('.js-num-retailers').html(__map_data.length);
	var latlng = new google.maps.LatLng(53.94205713184974, -1.5902698749999922);
  var mapOptions = { zoom: 6, center: latlng };
  map = new google.maps.Map(document.getElementById('map-canvas'), mapOptions);

	var infowindow = new google.maps.InfoWindow({maxWidth: 200});
 	

  for (i = 0; i < __map_data.length; i++) {
		var data = __map_data[i];
    marker = new google.maps.Marker({
        position: new google.maps.LatLng(data.lat, data.lng),
        map: map,
        icon: icon
    });

    google.maps.event.addListener(marker, 'click', (function (marker, i) {
        return function () {
        	var data2 = __map_data[i];
          infowindow.setContent(getContent(data2));
          infowindow.open(map, marker);
        }
    })(marker, i));
  }
}

google.maps.event.addDomListener(window, 'load', initialize);

function hasValue(val) {
	if(val && typeof val === 'number') return val;
	return (val && (typeof val === "string") && val.length > 0);
}

function getContent(data) {
	var address = '';

  if(data["Address 1"] && data["Address 1"].length > 0) {
    if(address && address.length !== 0) address += ",<br/>";
    address += data["Address 1"];
  }
    
  if(data["Address 2"] && data["Address 2"].length > 0) {
  	if(address && address.length !== 0) address += ",<br/>";
  	address += data["Address 2"];
  }

  if(data["City"] && data["City"].length > 0) {
  	if(address && address.length !== 0) address += ",<br/>";
  	address += data["City"];
  }

  if(data["Post Code"] && data["Post Code"].length > 0) {
  	if(address && address.length !== 0) address += ",<br/>";
  	address += data["Post Code"];
  }

  if(data["Country"] && data["Country"] !== data["Post Code"]) {
  	if(address && address.length !== 0) address += ",<br/>";
  	address += data["Country"];
  }

  var email = hasValue(data["Email"]) ? "<div>Phone: "+data["Email"]+"</div>" : "";
  var phone  = hasValue(data["Phone"]) ? "<div>Phone: "+data["Phone"]+"</div>" : "";
  var mobile = hasValue(data["Mobile"]) ? "<div>Mobile: "+data["Mobile"]+"</div>" : "";
  var website = hasValue(data["Website"]) ? "<div>Website: <a href='"+data["Website"]+"' target='_blank'>"+data["Website"]+"</a></div>" : "";

	return "<h3>"+data["Company Name"]+"</h3>"+
    		"<div class='retailerAddress'>"+address+"</div>"+
    		"<div class='retailerOther'>"+
    			email+
    			phone+
    			mobile+
    			website+
    		"</div>";
}


google.maps.event.addDomListener(window, 'load', function(){
	var $retailersList = $(".js-retailers-list");
	var $retailersLinks = $('.js-retailers-links')
	var groupedData = _.groupBy (__map_data, 'Country');

	for (var country in groupedData) {
		var id = country.replace(/ /g,'').toLowerCase().replace('&','');
		$retailersLinks.append("<li><a class='linkjumpto' href='#"+id+"'>"+country+"</a></li>");
		var retailersListInCountry = groupedData[country];
		var singPlur = retailersListInCountry.length > 1 ? 'retailers' : 'retailer';
		$retailersList.append("<h2 id='"+id+"'>"+retailersListInCountry.length + " "+ singPlur +" found in "+country+"</h2>");
		for (i = 0; i < retailersListInCountry.length; i++) {
			var data = retailersListInCountry[i];
			$retailersList.append("<li class='"+ (data.isPremium ? 'premium' : '') +"'>"+getContent(data)+"</li>");
		}
	}


	var headerHeight = $('.header.fixed').length !== 0 ? $('.header.fixed').height() : 0;

	$('a.linkjumpto').on('click',function (e) {
	    e.preventDefault();
	    var target = this.hash;
	    var $target = $(target);
	    $('html, body').stop().animate({
	        'scrollTop': $target.offset().top - headerHeight - 5
	    	}, 900, 'swing', function () {
	        window.location.hash = target;
	    });
	});

});


var scrollTrigger = 600, // px
  backToTop = function () {
      var scrollTop = $(window).scrollTop();
      if (scrollTop > scrollTrigger) {
          $('#back-to-top').addClass('show');
      } else {
          $('#back-to-top').removeClass('show');
      }
  };
backToTop();
$(window).on('scroll', function () {
  backToTop();
});
$('#back-to-top').on('click', function (e) {
  e.preventDefault();
  $('html,body').animate({
      scrollTop: 0
  }, 700);
});


